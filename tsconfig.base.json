{
  "compilerOptions": {
    "removeComments": false,
    "sourceMap": true,
    "strict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "moduleResolution": "node",
    "stripInternal": false,
    "importHelpers": true,
    "target": "es5",
    "outDir": "./.out",
    "lib": [
      "es5",
      "es2015",
      "dom"
    ]
  },
  "formatCodeOptions": {
    "indentSize": 2,
    "tabSize": 2
  }
}

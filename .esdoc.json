{
  "source": "./dist/esm",
  "destination": "./tmp/docs",
  "plugins": [
    {
      "name": "esdoc-standard-plugin",
      "options": {
        "title": "RxTask",
        "test": {
          "type": "mocha",
          "source": "./test",
          "includes": ["\\.test\\.ts"]
        }
      }
    },
    {
      "name": "esdoc-importpath-plugin",
      "option": {
        "stripPackageName": false,
        "replaces": [
          { "from": "^src/", "to": "" },
          { "from": "(?:esm|cjs)/", "to": "" },
          { "from": "/index\\.(?:ts|js)$", "to": "" }
        ]
      }
    }
  ]
}
